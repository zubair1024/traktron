{
    "name": "Caesar",
    "version": "1.131.0",
    "description": "ROAM Frontend UI",
    "private": true,
    "devDependencies": {
        "babel-core": "6.9.1",
        "babel-loader": "6.2.4",
        "babel-polyfill": "6.5.0",
        "babel-preset-es2015": "6.9.0",
        "babel-preset-stage-2": "6.5.0",
        "css-loader": "0.23.1",
        "grunt": "1.0.1",
        "grunt-cli": "1.2.0",
        "grunt-contrib-cssmin": "1.0.1",
        "grunt-contrib-jshint": "1.0.0",
        "grunt-contrib-less": "1.3.0",
        "grunt-contrib-qunit": "1.2.0",
        "grunt-contrib-uglify": "1.0.1",
        "grunt-contrib-watch": "1.0.0",
        "grunt-minjson": "^0.4.0",
        "grunt-run": "0.6.0",
        "html-loader": "0.4.3",
        "jscs": "3.0.3",
        "phantomjs": "2.1.7",
        "raw-loader": "0.5.1",
        "rimraf": "2.5.2",
        "script-loader": "0.7.0",
        "style-loader": "0.13.1",
        "updtr": "0.1.15",
        "webpack": "1.13.1",
        "webpack-dev-server": "1.14.1"
    },
    "scripts": {
        "setup": "npm install rimraf updtr grunt-cli webpack webpack-dev-server jscs -g",
        "install:clean": "rimraf node_modules && npm install",
        "start": "webpack-dev-server --inline --hot",
        "update": "updtr",
        "maps:clean": "rimraf js/maps/bundles",
        "maps:devbuild": "npm run maps:clean && webpack",
        "maps:watch": "npm run maps:clean && webpack -w",
        "maps:build": "npm run maps:clean && webpack --config webpack.production.config.js",
        "maps:jscs": "jscs js/maps"
    },
    "repository": {
        "type": "git",
        "url": "git.roamworks.com:roam"
    },
    "author": "Michael Klein, Glenn Dwiyatcita, Zubair Ahmed"
}
